    {
        "op": "core/column-addition",
        "description": "Create column ``INDX Ebook Central`` with the record's first value from ``Ebook Central ID`` if it's an ID and the ISBNs from the BIB cleanup project if it's not",
        "engineConfig": {
            "facets": [],
            "mode": "record-based"
        },
        "baseColumnName": "BIB Number",
        "expression": "grel:if(startsWith(cells[\"Ebook Central ID\"].value,\"null\"),cell.cross(\"UXU01_Cleanup\",\"BIB Number\")[0].cells[\"ISBN Value\"].value,row.record.cells[\"Ebook Central ID\"].value[0])",
        "onError": "set-to-blank",
        "newColumnName": "INDX Ebook Central",
        "columnInsertIndex": 1
    }
    {
        "op": "core/text-transform",
        "description": "Null values in column ``Ebook Central ID`` that are the same as that row's value in ``INDX Ebook Central``",
        "engineConfig": {
            "facets": [],
            "mode": "record-based"
        },
        "columnName": "Ebook Central ID",
        "expression": "grel:if(value==cells[\"INDX Ebook Central\"].value,null,value)",
        "onError": "keep-original",
        "repeat": false,
        "repeatCount": 10
    }
    {
        "op": "core/multivalued-cell-split",
        "description": "Divide values in column ``INDX Ebook Central`` into new rows at each `}|{`",
