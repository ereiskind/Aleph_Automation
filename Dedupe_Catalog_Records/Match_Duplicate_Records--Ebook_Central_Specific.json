                    "selectBlank": false,
                    "selectError": false
                }
            ],
            "mode": "record-based"
        },
        "columnName": "Ebook Central ID",
        "expression": "grel:\"null\"+toString(row.record.index+1)",
        "onError": "keep-original",
        "repeat": false,
        "repeatCount": 10
    },
    {
        "op": "core/column-move",
        "description": "Move column ``Record Number`` to beginning",
        "columnName": "Record Number",
        "index": 0
    }
    {
        "op": "core/row-reorder",
        "description": "Reorder rows based on ``Record Number``",
        "mode": "row-based",
        "sorting": {
            "criteria": [
                {
                    "valueType": "string",
                    "column": "Record Number",
                    "blankPosition": 2,
                    "errorPosition": 1,
                    "reverse": false,
                    "caseSensitive": false
