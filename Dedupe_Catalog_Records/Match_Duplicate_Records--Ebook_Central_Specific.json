    {
        "op": "core/column-addition",
        "description": "Create empty column ``Ebook Central BIB URLs``",
        "engineConfig": {
            "facets": [],
            "mode": "record-based"
        },
        "baseColumnName": "BIB URL",
        "expression": "grel:null",
        "onError": "set-to-blank",
        "newColumnName": "Ebook Central BIB URLs",
        "columnInsertIndex": 5
    },
    {
        "op": "core/text-transform",
        "description": "Copy Ebook Central URLs from ``BIB URL`` into ``Ebook Central BIB URLs``",
        "engineConfig": {
            "facets": [],
            "mode": "record-based"
        },
        "columnName": "Ebook Central BIB URLs",
        "expression": "grel:find(cells[\"BIB URL\"].value,/https?:\\/\\/ebookcentral\\.proquest\\.com\\/lib\\/[\\w-]*\\/detail\\.action\\?docID=\\d+/)[0]",
        "onError": "keep-original",
        "repeat": false,
        "repeatCount": 10
    }
    {
        "op": "core/text-transform",
        "description": "Copy MyiLibrary URLs from ``BIB URL`` into ``Ebook Central BIB URLs``",
        "engineConfig": {
            "facets": [],
            "mode": "record-based"
        },
        "columnName": "Ebook Central BIB URLs",
        "expression": "grel:find(cells[\"BIB URL\"].value,/https?:\\/\\/(lib|www)\\.myilibrary\\.com\\/?(detail\\.asp)?\\?(id|ID)=\\d+/)[0]",
        "onError": "keep-original",
        "repeat": false,
        "repeatCount": 10
    }
    {
        "op": "core/text-transform",
        "description": "Copy Ebrary URLs with `=` from ``BIB URL`` into ``Ebook Central BIB URLs``",
        "engineConfig": {
            "facets": [],
            "mode": "record-based"
        },
        "columnName": "Ebook Central BIB URLs",
        "expression": "grel:find(cells[\"BIB URL\"].value,/https?:\\/\\/site\\.ebrary\\.com\\/.*(id|ID)=\\d+/)[0]",
        "onError": "keep-original",
        "repeat": false,
        "repeatCount": 10
    }
    {
        "op": "core/text-transform",
        "description": "Copy Ebrary URLs without `=` from ``BIB URL`` into ``Ebook Central BIB URLs``",
        "engineConfig": {
            "facets": [],
            "mode": "record-based"
        },
        "columnName": "Ebook Central BIB URLs",
        "expression": "grel:find(cells[\"BIB URL\"].value,/https?:\\/\\/site\\.ebrary\\.com\\/id\\/\\d+/)[0]",
        "onError": "keep-original",
        "repeat": false,
        "repeatCount": 10
    }
    {
        "op": "core/text-transform",
        "description": "Copy Ebook Library URLs from ``BIB URL`` into ``Ebook Central BIB URLs``",
        "engineConfig": {
            "facets": [],
            "mode": "record-based"
        },
        "columnName": "Ebook Central BIB URLs",
        "expression": "grel:find(cells[\"BIB URL\"].value,/https?:\\/\\/(www.)?\\w*\\.eblib\\.com\\/[\\w\\/.?=&]*=\\w+/)[0]",
        "onError": "keep-original",
        "repeat": false,
        "repeatCount": 10
    }
    {
        "op": "core/multivalued-cell-join",
        "description": "Combine values in column ``Ebook Central BIB URLs`` for a record in the first row of the record seperated by pipes",
        "columnName": "Ebook Central BIB URLs",
        "keyColumnName": "HOL Number",
        "separator": "|"
    },
