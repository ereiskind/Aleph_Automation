            ],
            "mode": "record-based"
        },
        "columnName": "Title",
        "expression": "grel:value.unescape(\"html\").unescape(\"xml\").reinterpret(\"utf-8\")",
        "onError": "keep-original",
        "repeat": false,
        "repeatCount": 10
    }
