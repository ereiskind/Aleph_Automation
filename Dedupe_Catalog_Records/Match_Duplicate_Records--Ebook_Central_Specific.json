    {
        "op": "core/column-addition",
        "description": "Create empty column ``Ebook Central BIB URLs``",
        "engineConfig": {
            "facets": [],
            "mode": "record-based"
        },
        "baseColumnName": "BIB URL",
        "expression": "grel:null",
