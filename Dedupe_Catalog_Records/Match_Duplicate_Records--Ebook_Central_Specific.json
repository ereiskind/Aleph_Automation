        "engineConfig": {
            "facets": [],
            "mode": "record-based"
        },
        "columnName": "Ebook Central BIB URLs",
        "expression": "grel:find(cells[\"BIB URL\"].value,/https?:\\/\\/ebookcentral\\.proquest\\.com\\/lib\\/[\\w-]*\\/detail\\.action\\?docID=\\d+/)[0]",
        "onError": "keep-original",
