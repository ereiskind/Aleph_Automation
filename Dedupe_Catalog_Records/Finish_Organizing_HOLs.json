    {
        "op": "core/column-addition",
        "description": "Create column ``Combined`` with BIB and sublibrary code",
        "engineConfig": {
            "facets": [],
            "mode": "row-based"
        },
        "baseColumnName": "BIB Number",
        "expression": "grel:value+\"-\"+cells[\"HOL Sublibrary\"].value",
        "onError": "set-to-blank",
        "newColumnName": "Combined",
        "columnInsertIndex": 2
    },
    {
