        "description": "Create column ``Duplicate`` containing `Multiple HOL with same sublibrary` in rows where ``Combined`` is a duplicate",
        "engineConfig": {
            "facets": [
                {
                    "type": "list",
                    "name": "Combined",
                    "expression": "facetCount(value, 'value', 'Combined') > 1",
                    "columnName": "Combined",
                    "invert": false,
                    "omitBlank": false,
                    "omitError": false,
                    "selection": [
                        {
                            "v": {
                                "v": true,
                                "l": "true"
                            }
                        }
                    ],
                    "selectBlank": false,
                    "selectError": false
                }
            ],
            "mode": "row-based"
        },
        "baseColumnName": "HOL Sublibrary",
        "expression": "grel:\"Multiple HOL with same sublibrary\"",
        "onError": "set-to-blank",
        "newColumnName": "Duplicate",
        "columnInsertIndex": 3
    },
