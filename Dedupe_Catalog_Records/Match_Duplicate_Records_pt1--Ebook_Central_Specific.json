    {
        "op": "core/text-transform",
        "description": "For the rows with no values in ``Ebook Central BIB URL``, add `No URL` if there isn't an FSU URL for the BIB, the domain name with an asterisk in front if the domain can be identified, `Two domains, but first isn't the proxy` if two domains were found and the first didn't match the FSU proxy header, and `More than two domains` if three or more domains were found",
        "engineConfig": {
            "facets": [
                {
                    "type": "list",
                    "name": "Ebook Central BIB URLs",
                    "expression": "isBlank(value)",
                    "columnName": "Ebook Central BIB URLs",
                    "invert": false,
                    "omitBlank": false,
                    "omitError": false,
                    "selection": [
                        {
                            "v": {
                                "v": true,
                                "l": "true"
                            }
                        }
                    ],
                    "selectBlank": false,
                    "selectError": false
                }
            ],
            "mode": "row-based"
        },
        "columnName": "Ebook Central BIB URLs",
        "expression": "grel:if(isNull(cells[\"BIB URL 1\"].value),\"No URL\",\"*\"+if(length(find(cells[\"BIB URL 1\"].value,/https?:\\/\\/(\\w|\\.|-)*\\/?/))==1,find(cells[\"BIB URL 1\"].value,/https?:\\/\\/(\\w|\\.|-)*\\/?/)[0],if(length(find(cells[\"BIB URL 1\"].value,/https?:\\/\\/(\\w|\\.|-)*\\/?/))==2,if(inArray(slice(find(cells[\"BIB URL 1\"].value,/https?:\\/\\/(\\w|\\.|-)*\\/?/),0,1),\"https://login.proxy.lib.fsu.edu/\"),find(cells[\"BIB URL 1\"].value,/https?:\\/\\/(\\w|\\.|-)*\\/?/)[1],\"Two domains, but first isn't the proxy\"),\"More than two domains\")))",
        "onError": "keep-original",
        "repeat": false,
        "repeatCount": 10
    }
]