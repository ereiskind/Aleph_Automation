    {
        "op": "core/text-transform",
        "description": "Copy Ebook Central URLs from ``BIB URL`` into ``Ebook Central BIB URLs``",
        "engineConfig": {
            "facets": [
                {
                    "type": "list",
                    "name": "Ebook Central BIB URLs",
                    "expression": "isBlank(value)",
                    "columnName": "Ebook Central BIB URLs",
                    "invert": false,
                    "omitBlank": false,
                    "omitError": false,
                    "selection": [
                        {
                            "v": {
                                "v": true,
                                "l": "true"
                            }
                        }
                    ],
                    "selectBlank": false,
                    "selectError": false
                }
            ],
            "mode": "row-based"
        },
        "columnName": "Ebook Central BIB URLs",
        "expression": "grel:find(cells[\"BIB URL 1\"].value,/https?:\\/\\/ebookcentral\\.proquest\\.com\\/lib\\/[\\w-]*\\/detail\\.action\\?docID=\\d+/)[0]",
        "onError": "keep-original",
        "repeat": false,
        "repeatCount": 10
    }
